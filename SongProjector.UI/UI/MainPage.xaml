        <Grid Grid.Row="1" x:Name="ContentGrid">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="200" />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <Grid Grid.Column="0" Margin="0,10,5,0">
                <Grid.RowDefinitions>
                    <RowDefinition Height="auto" />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <muxc:NavigationViewItemHeader Content="Schedule" />
                <ListView x:Name="ScheduleListView" SelectionMode="Single" SelectionChanged="ScheduleListView_SelectionChanged"
                        CanReorderItems="True"
                        CanDragItems="True"
                        AllowDrop="True"
                        Grid.Row="1">
                    <ListView.ItemTemplate>
                        <DataTemplate xmlns:media="using:SongProjector.Media" x:DataType="media:IMedia">
                            <TextBlock Text="{x:Bind DisplayName}" />
                        </DataTemplate>
                    </ListView.ItemTemplate>
                </ListView>
            </Grid>
            <Grid Grid.Column="1" Margin="5,10,0,0" ScrollViewer.VerticalScrollMode="Auto">
                <Grid Background="{ThemeResource LayerFillColorDefaultBrush}" BorderBrush="{ThemeResource CardStrokeColorDefaultBrush}" BorderThickness="1" CornerRadius="8,0,0,0">
                    <preview:PreviewSubPage x:Name="PreviewPage" />
                </Grid>
                <Grid VerticalAlignment="Bottom" Padding="10" Visibility="Collapsed"
                    Background="White" BorderBrush="{ThemeResource CardStrokeColorDefaultBrush}" BorderThickness="2" CornerRadius="8,8,0,0">
                    <MediaPlayerElement AreTransportControlsEnabled="True" MaxHeight="200" />
                </Grid>
            </Grid>
        </Grid>
    </Grid>
